        }

        repositionCloseSideBar = function() {
            $('#left-column').removeClass("show-for-large-up show-for-medium-up").hide();
            $('#middle-column').removeClass('large-9');
            $('#middle-column').addClass('large-12').addClass('medium-12');
        }
    </script>

