	            	rotation: 30
	            }
	        },
	        yAxis: {
	            max: jsondata.y_axis.max,
	            min: jsondata.y_axis.min,
	            offset: jsondata.y_axis.offset,
	            labels: {
	            	steps: jsondata.y_axis.steps
	            },
	            title: {
	            	enabled: false
	            }
	        },
	        tooltip: {
	        	formatter: function (){
	        		return this.key.replace("#val#", this.y);
	        	}
	        },
	        legend: {
	        	align: 'top',
	            itemDistance: 50
	        },
	        series: series
	    });
	});

